iconPath {
  border-radius: 100rpx;
}
.map_view{
  display: flex;
  flex-direction: column;
}

.map_text {
  flex-direction: row;
  background: #ededed;
  display: flex;
  /* align-items: center; */
  position: absolute;
  /* justify-content: center; */
  font-size: 28rpx;
  margin-top: 20rpx;
  border-radius: 15rpx;
  z-index: 10000;
  right: 10%;
  /* bottom: 6%; */
}
.map_text view{
  display:flex;
  flex-direction: column;
}
.map_text image{
  width: 60rpx;
  height: 60rpx;
}
